{
  "release-type": "node",
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "draft": true,
  "separate-pull-requests": true,
  "packages": {
    "packages/milliejs": {
      "component": "milliejs"
    },
    "packages/milliejs-core": {
      "component": "@milliejs/core"
    },
    "packages/milliejs-jest-utils": {
      "component": "@milliejs/jest-utils"
    },
    "packages/milliejs-store-base": {
      "component": "@milliejs/store-base"
    },
    "packages/milliejs-store-google-pubsub": {
      "component": "@milliejs/google-pubsub"
    },
    "packages/milliejs-store-memory": {
      "component": "@milliejs/store-memory"
    }
  },
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "milliejs"
    },
    {
      "type": "sentence-case"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
